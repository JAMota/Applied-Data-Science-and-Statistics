dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 10
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type3[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 100
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type4[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 1000
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type5[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 10000
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type6[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 100000
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type7[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 1000000
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type8[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 10000000
}
if(dataCyberSecuritySurvey2018TidyNameSizeCyber$type9[i] == 1){
dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] = dataCyberSecuritySurvey2018TidyNameSizeCyber$attackTypes[i] + 100000000
}
