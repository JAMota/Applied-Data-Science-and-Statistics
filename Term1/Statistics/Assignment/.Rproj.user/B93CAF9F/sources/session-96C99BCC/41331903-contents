#ex 2

x = c(7.4, 6.0, 8.6, 4.5, 2.0, 7.9, 4.0,
  2.6, 5.9, 7.3, 7.3, 7.0, 6.3, 8.1,
  7.1, 7.3, 6.6, 5.2, 9.8, 8.0, 10.9,
  6.3, 3.8, 5.0, 8.0, 10.7, 9.7, 6.0,
  6.8, 10.3, 7.6, 6.5, 7.1, 5.8, 6.9)


#xbar -z{0,975}*sigma/sqrt(n)
mean(x)-qnorm(0.975,0.1)*1.9/sqrt(35)


#xbar +z{0,975}*sigma/sqrt(n)
mean(x)+qnorm(0.975,0.1)*1.9/sqrt(35)



#b)

mean(x)-qt(0,975)*sd(x)/sqrt(35)


mean(x)+qt(0,975)*sd(x)/sqrt(35)